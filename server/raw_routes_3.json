components:
  parameters:
    x-traceability-id:
      name: x-traceability-id
      description: Common header parameters for centeralized logging.
      in: header
      required: true
      schema:
        type: string
        example: p0kkv9
    x-correlation-id:
      name: x-correlation-id
      description: >-
        Unique correlation ID which is either passed by consumer or generated as
        UUID.
      in: header
      required: true
      schema:
        type: string
        example: 4hjm0k
    x-auth-token:
      name: Authorization
      description: OAuth2 token generated by OKTA.
      in: header
      required: true
      schema:
        type: string
        example: >-
          Bearer
          eyJraWQiOiJWdTVKWEpJaUhhcnFqX3drUGtCSUlKd1ZFeGxHTy0yLTlGaXBTUm8ySUtVIiwiYWxnIjoiUlMyNTYifQ..dZoppdGWiqoXGiaESzyFyUcSsUl7tqvfQZ7Py_kTzEgXvIC54_-wLkiWhMfQKsbZfvS4HwWwF_s6E6Ot4ZAEqxIoDoKwpxOZtyiJpkOfbO13bgHH83sXWPxCHFMBcb_xKfe_gpGU_D0jeuWM-UEr3401sThXaj7VaM76L5Eua43dvdGfuXxMdM820mkfehmBike62KRtrGgtD8z5L62NCXUdZNwwNuO6pMu5OZ0W6r67m0iUt1j1BsI5Bw6t4ICSFj7TzrH77Qv-pdE9jHt6dmtp5pRmwYH__Vqj3lMBNSO-qzolUhhcT8m3m4iuvSaL0Tk1t7WsI5m6HbpRxankYA
    policyNumbers:
      name: policyNumbers
      description: >-
        Serves as the list of reference numbers. Comma separated for multiple
        reference numbers
      in: header
      required: true
      schema:
        type: string
        example: 123,456,789
    optimized:
      name: optimized
      description: >-
        This identifies if the return value should be optimized or not. If
        parameter is not defined, return the DetailedPoliciesInfo
      in: query
      required: false
      schema:
        type: boolean
    referenceNo:
      name: referenceNo
      description: Serves as the referenceNo
      in: path
      required: true
      schema:
        type: string
        example: Swp3iXOXSpKMKAT29rPam
    sequenceNumber:
      name: sequenceNumber
      description: Beneficiary sequence number generated by Ingenium Webservice
      in: path
      required: true
      schema:
        type: string
        example: Swp3iXOXSpKMKAT29rPam
    companyCode:
      name: companyCode
      description: >-
        Company code to determine which Ingenium Webservice will be called. e.g.
        SLOCPI, SLGFI
      in: query
      required: true
      schema:
        type: string
        example: SLOCPI
    companyCodeHeader:
      name: companyCode
      description: >-
        Company code to determine which Ingenium Webservice will be called. e.g.
        SLOCPI, SLGFI
      in: header
      required: false
      schema:
        type: string
        example: SLOCPI
    effectiveDateHeader:
      name: effectiveDate
      description: Effective date of request
      in: header
      required: false
      schema:
        type: string
        example: '2019-09-04'
    transactionAmountHeader:
      name: transactionAmount
      description: Transaction amount
      in: header
      required: false
      schema:
        type: string
        example: '6000'
    transactionFeeHeader:
      name: transactionFee
      description: Transaction fee
      in: header
      required: false
      schema:
        type: string
        example: '50'
    correlation-id:
      name: correlation-id
      description: Serves as the transaction id that is used for logging.
      in: header
      required: true
      schema:
        type: string
        example: Swp3iXOXSpKMKAT29rPam
    version:
      name: version
      description: >-
        Version of the API's service implementation. Defaults to "0" if not
        passed.
      in: query
      required: false
      schema:
        type: string
        example: 1
    relTypeCd:
      name: relTypeCd
      description: RelTypeCd for assignee
      in: query
      required: false
      schema:
        type: string
        example: A
        maxLength: 2
  requestBodies:
    PostBeneficiaryRequest:
      content:
        application/json:
          schema:
            $ref: '#/components/schemas/PostBeneficiaryRq'
    PutBeneficiaryRequest:
      content:
        application/json:
          schema:
            $ref: '#/components/schemas/PutBeneficiaryRq'
    PostVULWithdrawRequest:
      content:
        application/json:
          schema:
            $ref: '#/components/schemas/PostVULWithdrawRq'
    PostVULSwitchRequest:
      content:
        application/json:
          schema:
            $ref: '#/components/schemas/PostVULSwitchRq'
    PostDisbursementRequest:
      content:
        application/json:
          schema:
            $ref: '#/components/schemas/PostDisbursementRq'
    PostCtaRequest:
      content:
        application/json:
          schema:
            $ref: '#/components/schemas/PostCtaRq'
    PostEligibilityRequest:
      content:
        application/json:
          schema:
            type: object
            required:
              - companyCode
              - infoToCheck
              - info
            properties:
              companyCode:
                type: string
                example: SLGFI
              infoToCheck:
                type: string
                enum:
                  - NATIONALITY
                  - PRESENT_ADDRESS
                  - PERMANENT_ADDRESS
                  - OCCUPATION
                  - NATURE_OF_BUSINESS
                  - BUSINESS_ADDRESS
                example: PRESENT_ADDRESS
              info:
                anyOf:
                  - $ref: '#/components/schemas/AddressInfo'
                  - $ref: '#/components/schemas/NationalityInfo'
                  - $ref: '#/components/schemas/OccupationInfo'
                  - $ref: '#/components/schemas/NatureOfBusinessInfo'
          examples:
            PRESENT_ADDRESS:
              value:
                companyCode: SLGFI
                infoToCheck: PRESENT_ADDRESS
                info:
                  province: MM
                  city: MM
                  zip: '1234'
                  country: PH
                  planCode: RA1P0B
            PERMANENT_ADDRESS:
              value:
                companyCode: SLGFI
                infoToCheck: PERMANENT_ADDRESS
                info:
                  province: MM
                  city: MM
                  zip: '1234'
                  country: PH
                  planCode: RA1P0B
            NATIONALITY:
              value:
                companyCode: SLGFI
                infoToCheck: NATIONALITY
                info:
                  citizenship: PH
                  planCode: RA1P0B
            OCCUPATION:
              value:
                companyCode: SLGFI
                infoToCheck: OCCUPATION
                info:
                  occupation: ADMINISTRATOR
                  planCode: RA1P0B
            NATURE_OF_BUSINESS:
              value:
                companyCode: SLGFI
                infoToCheck: NATURE_OF_BUSINESS
                info:
                  natureOfBusiness: Banking
            BUSINESS_ADDRESS:
              value:
                companyCode: SLGFI
                infoToCheck: PRESENT_ADDRESS
                info:
                  province: MM
                  city: MM
                  zip: '1234'
                  country: PH
                  planCode: RA1P0B
    PostPolicyRequest:
      content:
        application/json:
          schema:
            $ref: '#/components/schemas/CreatePolicyDto'
    PostAutoAppraisalRequest:
      content:
        application/json:
          schema:
            $ref: '#/components/schemas/AutoAppraisalDto'
  schemas:
    PostBeneficiaryRs:
      title: Add beneficiary response object
      type: object
      properties:
        sequenceNumber:
          type: string
    DeleteBeneficiaryRq:
      type: object
      properties:
        policyNumber:
          type: string
          example: '03193849'
        sequenceNumber:
          type: string
          example: '027'
    PutBeneficiaryRq:
      type: object
      properties:
        policyNumber:
          type: string
          example: 30331822
        companyCode:
          type: string
          example: SLOCPI
        beneficiaryList:
          type: array
          items:
            allOf:
              - $ref: '#/components/schemas/NewBeneficiaryDto'
              - type: object
                properties:
                  sequenceNumber:
                    type: string
                    example: '027'
    PostBeneficiaryRq:
      title: Add beneficiary request parameter
      type: object
      properties:
        policyNumber:
          type: string
          example: 30331822
        companyCode:
          type: string
          example: SLOCPI
        beneficiaryList:
          type: array
          items:
            $ref: '#/components/schemas/NewBeneficiaryDto'
    PostCtaRq:
      title: CTA Tagging request parameter
      type: object
      properties:
        adminSysClientNo:
          type: string
          example: 30331822
        companyCode:
          type: string
          example: SLOCPI
        policyCurrency:
          type: string
          example: PS
        ctaType:
          type: string
          example: T
        taggingType:
          type: string
          example: SAVE
    PostDisbursementRq:
      title: Disbursement request parameter
      type: object
      properties:
        policyNumber:
          type: string
          example: 30331822
        companyCode:
          type: string
          example: SLOCPI
        policyCurrency:
          type: string
          example: PS
        clientOwnerName:
          type: string
          maxLength: 90
          example: John Doe
        disbursementAmount:
          type: string
          example: '7000.00'
        approver:
          type: string
          example: ''
        transactionType:
          type: string
          example: VUL3
        disbursementType:
          type: string
          example: C
    PostVULSwitchRq:
      title: VUL Switch request parameter
      type: object
      properties:
        policyNumber:
          type: string
          example: 30331822
        companyCode:
          type: string
          example: SLOCPI
        policyCurrency:
          type: string
          example: PS
        switchMethod:
          type: string
          example: amount | percentage
        switchToNewFund:
          type: object
          nullable: true
          properties:
            planId:
              type: string
              example: FLRP1B
            policyStatusCd:
              type: string
              example: '1'
            fundContractStatusCd:
              type: string
              example: A
            coverageStatusCd:
              type: string
              example: '1'
            fundCounter:
              type: array
              items:
                type: string
                example: '001'
            fundId:
              type: array
              items:
                type: string
                example: SLPBA
            fundStatus:
              type: array
              items:
                type: string
                example: A
        transferCharge:
          type: string
          example: '57.5'
        effectiveDate:
          type: string
          example: '2019-09-10'
        fundCounter:
          type: array
          items:
            type: string
            example: '001'
        fundId:
          type: array
          items:
            type: string
            example: abc
        switchFromAmount:
