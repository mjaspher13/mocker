openapi: 3.0.0
info:
  version: 1.3.2
  title: Policy Service API
  description: >-
    Retrieves policy details, both VUL and trad, from Ingenium DB, Ingenium
    Webservice and Client DB.
  contact:
    name: ASCP Common API Team
    email: ASCP-Common-API-Services-Team@sunlife.com
    url: >-
      https://sunlifefinancial.sharepoint.com/sites/Operations/ASCPDA/ASCProductManagement/SitePages/slf-lif.aspx
  x-api-id: '0403'
  x-rapidapi-team: PH-Common-APIs-Team
  x-business-domain: Cross-Cutting Platforms
  x-region: SLF Asia
  x-api-type: System
  x-journey-stage: Interaction Omni-Channel
  x-country: PH
externalDocs:
  description: Policy Service Bitbucket Link
  url: http://sv516072.ph.sunlife:7990/projects/COM/repos/slf-life-svc/browse
servers:
  - description: SwaggerHub API Auto Mocking
    url: >-
      https://virtserver.swaggerhub.com/Sun-Life-Financial/policy-service-api/1.0.0
  - url: https://sv84959.sunlifecorp.com:34443/dev-l4j-ph-policy-service
    description: DEV server environment
  - url: https://sv85333.sunlifecorp.com:34443/sit-l4j-ph-policy-service
    description: SIT server environment
  - url: https://sv85185.sunlifecorp.com:34443/uat-l4j-ph-policy-service
    description: UAT server environment
  - url: https://sv85215.sunlifecorp.com:34443/l4j-ph-policy-service
    description: PROD 1 server environment
  - url: https://sv85216.sunlifecorp.com:34443/l4j-ph-policy-service
    description: PROD 2 server environment
  - url: https://de-servicing.sunlifecorp.com/l4j-ph-policy-service
    description: PROD F5 server environment
tags:
  - name: life-service-controller
    description: Life Service Controller
    externalDocs:
      description: ''
      url: ''
  - name: policy-details-controller
    description: Endpoints that retrieves policy details from Client DB
    externalDocs:
      description: ''
      url: ''
  - name: policy-service-controller
    description: Endpoints that retrieves policy details from Ingenium DB/Webservice
    externalDocs:
      description: ''
      url: ''
  - name: policy-eligibility-controller
    description: Endpoint that checks for eligibility via Ingenium DB/Webservice
    externalDocs:
      description: ''
      url: ''
  - name: policy-creation-controller
    description: Endpoints that creates a new policy into Ingenium DB/Webservice
    externalDocs:
      description: ''
      url: ''
paths:
  /health-check:
    get:
      tags:
        - life-service-controller
      summary: Health check
      description: Health check api for Policy service
      operationId: healthCheckUsingGET
      parameters:
        - $ref: '#/components/parameters/x-traceability-id'
        - $ref: '#/components/parameters/x-correlation-id'
        - $ref: '#/components/parameters/x-auth-token'
      responses:
        '200':
          description: OK
          content:
            application/json:
              schema:
                type: object
              example:
                dbHealthCheck:
                  PASSED:
                    - CLI_PRTFO_LF_T TABLE
                    - CLI_PRTFO_LF_STAT TABLE
                    - CLI_BNFY_LF_T TABLE
                    - CLI_CVG_LF_T TABLE
                    - TCVG
                    - TEDIT
                    - TFA
                slocipiIngeniumHealthCheck: true
                slgfiIngeniumHealthCheck: false
        '400':
          description: Request validation error
        '401':
          description: Unauthorized
        '403':
          description: Forbidden
        '404':
          description: Not Found
        '500':
          description: Internal server error
      deprecated: false
      security:
        - dev-api-scope:
            - apiplatform.asia.policyservice.policy.read
  /policy/trad/details/{referenceNo}:
    get:
      tags:
        - policy-details-controller
      summary: Retrieves policy Trad details
      description: Retrieves policy Trad details
      operationId: getPolicyDetailsTradUsingGET
      parameters:
        - $ref: '#/components/parameters/x-traceability-id'
        - $ref: '#/components/parameters/x-correlation-id'
        - $ref: '#/components/parameters/x-auth-token'
        - $ref: '#/components/parameters/version'
        - $ref: '#/components/parameters/correlation-id'
        - $ref: '#/components/parameters/referenceNo'
      responses:
        '200':
          description: Successful
          content:
            '*/*':
              schema:
                $ref: '#/components/schemas/PolicyDetailsTradDto'
        '400':
          description: Request Validation Error
        '401':
          description: Unauthorized
        '403':
          description: Forbidden
        '404':
          description: Not Found
        '500':
          description: Internal Server Error
      deprecated: false
      security:
        - dev-api-scope:
            - apiplatform.asia.policyservice.policy.read
  /policy/vul/details/{referenceNo}:
    get:
      tags:
        - policy-details-controller
      summary: Retrieves policy VUL details
      description: Retrieves policy VUL details
      operationId: getPolicyDetailsVulUsingGET
      parameters:
        - $ref: '#/components/parameters/x-traceability-id'
        - $ref: '#/components/parameters/x-correlation-id'
        - $ref: '#/components/parameters/x-auth-token'
        - $ref: '#/components/parameters/version'
        - $ref: '#/components/parameters/correlation-id'
        - $ref: '#/components/parameters/referenceNo'
      responses:
        '200':
          description: Successful
          content:
            '*/*':
              schema:
                $ref: '#/components/schemas/PolicyDetailsVulDto'
        '400':
          description: Request Validation Error
        '401':
          description: Unauthorized
        '403':
          description: Forbidden
        '404':
          description: Not Found
        '500':
          description: Internal Server Error
      deprecated: false
      security:
        - dev-api-scope:
            - apiplatform.asia.policyservice.policy.read
